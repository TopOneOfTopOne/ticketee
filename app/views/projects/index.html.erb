<div class="header">
  <h1>The index baby
    <% admin_only do %>
      <%= link_to "New Project", new_admin_project_path, class: "new" %>
    <% end %>
  </h1>
</div>
<% @projects.each do |project| %>
  <ul>
    <h2><%= link_to project.name, project, id: "projects" %></h2>
    <p>
      <%= project.description %>
    </p>
  </ul>
<% end %>
